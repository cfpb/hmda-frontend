{
  "name": "hmda-frontend",
  "version": "0.0.1",
  "description": "Collection of HMDA frontend apps",
  "main": "./src/index.js",
  "engines": {
    "node": ">=12.x"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "yarn run cypress run",
    "ci": "REACT_APP_ENVIRONMENT=CI yarn start -p 3000",
    "ci-data": "yarn newman run cypress/ci/tests/CREATE_INSTITUTIONS.postman_collection.json -e cypress/ci/config/PLATFORM_ENV.postman_environment.json -d cypress/ci/config/institutions.json"
  },
  "keywords": [
    "HMDA"
  ],
  "author": "Consumer Financial Protection Bureau",
  "license": "CC0-1.0",
  "dependencies": {
    "csv-parse": "4.6.3",
    "detect-browser": "4.7.0",
    "fast-deep-equal": "2.0.1",
    "file-saver": "2.0.2",
    "filereader-stream": "2.0.0",
    "keycloak-js": "4.5.0",
    "mapbox-gl": "1.5.0",
    "markdown-to-jsx": "6.11.4",
    "normalize.css": "8.0.1",
    "react": "16.10.1",
    "react-app-polyfill": "1.0.6",
    "react-dom": "16.10.1",
    "react-dropzone": "10.1.10",
    "react-helmet": "5.2.1",
    "react-redux": "7.1.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.3",
    "react-select": "3.1.0",
    "react-window": "1.8.5",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "split2": "3.1.1"
  },
  "browserslist": [
    "Chrome >= 60",
    "Safari >= 10.1",
    "Firefox >= 54",
    "Edge >= 15",
    "ie >= 11"
  ],
  "devDependencies": {
    "@cypress/skip-test": "2.5.0",
    "@testing-library/cypress": "6.0.0",
    "cypress": "4.10.0",
    "cypress-file-upload": "3.5.3",
    "cypress-keycloak": "1.5.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint-plugin-prettier": "3.1.1",
    "http-proxy-middleware": "0.19.1",
    "newman": "5.1.2",
    "redux-devtools-extension": "2.13.8",
    "serialize-javascript": "5.0.1"
  },
  "proxy": "http://localhost:8080"
}
